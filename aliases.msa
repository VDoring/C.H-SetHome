/sethome = >>>
        store_value(player() . 'homeloc', ploc())
        msg('§6Set Home!')
<<<

/home = >>>
    @home = get_value(player() . 'homeloc')
    if(@home == null){
        msg('§cHome is not specified.')
    }
    else {
        set_ploc(@home)
    }
<<<

/delhome =>>>
	clear_value(player() . 'homeloc')
	msg('§6Delete Hoem Success!')
<<<
